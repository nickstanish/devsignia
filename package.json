{
  "name": "devsignia",
  "version": "1.0.0",
  "description": "devsignia is a chrome extension for differentiating dev and prod sites so you always know which environment you're in.",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "npm run clean && webpack --config=webpack.config.js --env production",
    "start": "npm run clean && webpack serve --config=webpack.config.js",
    "clean": "rm -rf public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickstanish/devsignia.git"
  },
  "author": "Nick Stanish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickstanish/devsignia/issues"
  },
  "homepage": "https://github.com/nickstanish/devsignia#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^5.3.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
